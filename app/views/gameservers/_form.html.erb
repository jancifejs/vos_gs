<%= form_for(@gameserver) do |f| %>
    <fieldset class="form-horizontal">




      <div class="form-group title">
        <%= f.label :title,class:"control-label col-sm-2" %>
        <div class="col-sm-10">
          <%= f.text_field :name,class:"form-control" %>
        </div>
      </div>
      <div class="form-group title">
        <%= f.label :ip,class:"control-label col-sm-2" %>
        <div class="col-sm-10">
          <%= f.text_field :ip,class:"form-control" %>
        </div>
      </div>
      <div class="form-group title">
        <%= f.label :port,class:"control-label col-sm-2" %>
        <div class="col-sm-10">
          <%= f.text_field :port,class:"form-control" %>
        </div>
      </div>

      <div class="form-group actions">
        <div class="col-sm-offset-2 col-sm-10">
          <%= f.submit "Save", class: "btn btn-primary" %>
        </div>
      </div>
    </fieldset>


<% end %>
